{
  "name": "tsadmin-mqtt",
  "version": "1.1.4",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "windows": "ng build --configuration=windows --base-href ./ && electron . ",
    "linux": "ng build --configuration=linux --base-href ./ && electron . ",
    "macOS": "ng build --configuration=macOS --base-href ./ && electron . ",
    "arm7l": "ng build --configuration=armv7l --base-href ./ && electron . ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prebuild": "npm --no-git-tag-version version patch",
    "test": "ng test",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "electron-package-windows": " npm run prebuild && ng build --configuration=windows && electron-packager . TSAdminMQTT --platform=win32 --arch=x64 --asar --overwrite --icon=./src/favicon.ico --our=TSAdminMQTT_Windows --ignore=^/.angular",
    "electron-package-linux": " npm run prebuild && ng build --configuration=linux && electron-packager . TSAdminMQTT --platform=linux --arch=x64 --asar --overwrite --icon=./src/favicon.ico --our=TSAdminMQTT_Linux --ignore=^/.angular",
    "electron-package-macOS": " npm run prebuild && ng build --configuration=macOS && electron-packager . TSAdminMQTT --platform=darwin --arch=x64 --asar --overwrite --icon=./src/favicon.ico --our=TSAdminMQTT_MacOS --ignore=^/.angular",
    "electron-package-arm7l": " npm run prebuild && ng build --configuration=linux && electron-packager . TSAdminMQTT --platform=linux --arch=armv7l --asar --overwrite --icon=./src/favicon.ico --our=TSAdminMQTT_Linux_arm --ignore=^/.angular",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "aedes": "^0.47.0",
    "aedes-packet": "^3.0.0",
    "aedes-persistence": "^9.1.2",
    "axios": "^0.27.2",
    "bulk-write-stream": "^2.0.1",
    "delay": "^5.0.0",
    "electron-dl": "^3.3.1",
    "electron-packager": "^16.0.0",
    "ip": "^1.1.8",
    "local-devices": "^3.2.0",
    "ngx-indexed-db": "^11.0.2",
    "ngx-mqtt": "^9.0.5",
    "ngx-toastr": "^15.0.0",
    "ngx-touch-keyboard": "^2.2.0",
    "node": "^14.17.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "websocket-stream": "^5.5.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular-eslint/builder": "14.0.3",
    "@angular-eslint/eslint-plugin": "14.0.3",
    "@angular-eslint/eslint-plugin-template": "14.0.3",
    "@angular-eslint/schematics": "14.0.3",
    "@angular-eslint/template-parser": "14.0.3",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "electron": "^20.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  }
}
